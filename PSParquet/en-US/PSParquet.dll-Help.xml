    <command:details>
      <command:name>Export-Parquet</command:name>
      <command:verb>Export</command:verb>
      <command:noun>Parquet</command:noun>
      <maml:description>
        <maml:para>Export objects to Parquet file</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>Exports an array of objects to a parquet file. Make sure your objects are formatted correctly. Array, List and HashTable parameters are not supported. Convert these types to basic types before running Export-Parquet.</maml:para>
    </maml:description>
